	  REFERENCES "SUIM"."HOUSE" ("HOUSE_NO") ON DELETE CASCADE ENABLE;
--------------------------------------------------------
--  Ref Constraints for Table RATTACHMENT
--------------------------------------------------------

  ALTER TABLE "SUIM"."RATTACHMENT" ADD FOREIGN KEY ("REPORT_NO")
	  REFERENCES "SUIM"."REPORT" ("REPORT_NO") ON DELETE CASCADE ENABLE;
--------------------------------------------------------
--  Ref Constraints for Table REPORT
--------------------------------------------------------

  ALTER TABLE "SUIM"."REPORT" ADD FOREIGN KEY ("MEMBER_ID")
	  REFERENCES "SUIM"."MEMBER" ("MEMBER_ID") ON DELETE CASCADE ENABLE;
